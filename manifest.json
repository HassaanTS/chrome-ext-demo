{
    "manifest_version": 2,
    "name": "Github Table Summary",
    "version": "0.1.0",
    "description": "Calculate the total size of beat objectives and generate a summary",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/icon.png"
        }
    },
    "permissions": [
        "*://github.com/*/*",
        "tabs"
    ]
}